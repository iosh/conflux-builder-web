
# LOG_LEVEL option ：trace | debug | info | warn | error | fatal | silent（default：info）
LOG_LEVEL=info

# Require ：GITHUB_TOKEN, GITHUB_WEBHOOK_SECRET, DB_FILE_NAME
# GitHub Configuration
GITHUB_TOKEN=your_github_token_here

# GitHub Webhook Configuration  
# This secret is used to verify webhook payloads from GitHub
# Generate a random secret and configure it in your GitHub repository settings
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# SQLite database 
DB_FILE_NAME=db.sqlite